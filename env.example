VITE_PUBLIC_API_URL=https://movie-dubie-api-dev-cjcivtah.uc.gateway.dev

VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

## Google Cloud Workflows
# Google Cloud project ID for Workflows API
VITE_GOOGLE_CLOUD_PROJECT=
# Google Cloud location/region for Workflows API (e.g., us-central1)
VITE_GOOGLE_CLOUD_LOCATION=

## Admin UI Config loading
# When true, preload UI configs from a local JSON file instead of the API
VITE_ADMIN_UI_CONFIG_FROM_LOCAL=false
# Path to local JSON file (served by Vite/public). Default: /ui-config.json
VITE_ADMIN_UI_CONFIG_LOCAL_PATH=/ui-config.json

## Note: Vite dev server port is configured in vite.config.ts (default 5173)

## Cloudflare Image Transformations
# Set Cloudflare domain for absolute CDN URLs (required for image transformations)
VITE_CLOUDFLARE_DOMAIN=4hum.ai
# Enable/disable CDN functionality (default: true)
VITE_ENABLE_CDN=true
# Enable/disable image transformations (default: true)
VITE_ENABLE_IMAGE_TRANSFORMATIONS=true